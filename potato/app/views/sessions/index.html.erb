<div class="container">
  <div class="row" id ="headbar">
          <div class="col-md-4">
          <h2 class="text-primary">🥔 potato.bond</h2>
          </div>
          <div class="col-lg-6">
          <p class="row" id="user-data">

          </p>
          </div>
          <div class="col-sm-2">
          <p class="text text-right"><button id="sign-out" class="btn btn-outline-secondary">Sign Out</button></p>
          </div>
          <hr class="text-primary border-3 opacity-50">
  </div>
  <div class="row" id="sign-in">
          <p> <span class="text-seconday"> <b>leverages local coincidences of needs and wants for change</b></span><br>
          potato.bond aims to be <br> permissionless infrastructure <br> in service of <br> locally aware and globally conscious <br> modes of production, distribution and ownership
          </p>
          <hr class="text-primary border-3 opacity-50">

          <button id="sign-in-button" class='btn btn-outline-primary'><img src="./siwe.png"></button>

  </div>
  <!---  body start --->

  <div class="row d-none" id="main-wrapper">
    <div class="accordion" id="accordionExample">
        <div class="accordion-item" id="globalstate">
          <h2 class="accordion-header" id="headingOne">
            <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
              Global State
            </button>
          </h2>
          <div id="collapseOne" class="accordion-collapse collapse show" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
            <div class="accordion-body">
                <div class="row">
                        <div class="col-8">
                          <p>Total Constituent Simples: <span id='gid'> <%= @gid  %> </span></p> <br>
                          <p>Total Minted Baskets: <span id='gid'> <%= @basket_nr  %> </span></p> <br>
                          <p>Total Registered Farmers: <span id='gid'> <%= @farmers_nr  %> </span></p> <br>
                          <p>Total Number of Unique Areas: <span id='gid'> <%= @area_nr  %> </span></p> <br>
                        </div>

                </div>
            </div>
          </div>
        </div>
        <div class="accordion-item" id="userdata">
            <h2 class="accordion-header" id="headingTwo">
              <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                My Data
              </button>
            </h2>
            <div id="collapseTwo" class="accordion-collapse collapse" aria-labelledby="headingTwo" data-bs-parent="#accordionExample">
            <div class="accordion-body">
                <div class="row">
                  <div class="col-4">
                    <div class="row">
                          <h6>
                            Farmer: <% if @farmer %>  <a href= <%= "ipfs://#{@farmer.data_url}" %> >data</a> <% else %> "Not Farmer" <% end %>
                          </h6>
                    </div>
                    <div class="row">
                        <h6>
                          Consumer:  <% if @consumer %>  <a href= <%= "ipfs://#{@consumer.data_url}" %>>data</a> <% else %> "No Consumer Profile" <% end %>
                        </h6>
                    </div>
                    <div class="row">
                      <h6>
                        Governed Areas: <% if @area %>  <a href= <%= "ipfs://#{@area.data_url}" %>>data</a> <% else %> "No Areas Goverened" <% end %>
                      </h6>
                    </div>
                  </div>
                  <div class="col-8">
                  <h6> Owned Baskers </h6>
                  <hr class="text-primary border-1">
                    <div class="row">
                    <div class="col-4">
                      <p>As consumer</p>
                      <hr>
                      <% if @baskets_consumer %> 
                      <% @baskets_consumer.each do |b| %>
                      <p>   
                        <a href=<%= "#{b.data_url}" %>> data </a> | nft_id : <%= b.nft_id %> | 
                        <a href=<%="ipfs://#{Farmer.find_by(address: b.farmer_address).data_url}"%>> farmer </a> 
                      </p>
                        <% end %>
                        <% else %> 
                          <%= "No Baskets Owned" %> 
                        <% end %>
                      </div>
                    <div class="col-4">
                    <p>As farmer</p>
                    <hr
                    <% if @baskets_farmer %> 
                    <% @baskets_farmer.each do |b| %>
                    <p>   
                      <a href=<%= "#{b.data_url}" %>> data </a> | nft_id : <%= b.nft_id %> 
                    </p>
                      <% end %>
                      <% else %> 
                        <%= "No Baskets Owned" %> 
                      <% end %>
                    </div>

                    </div>
                  </div>
                </div>
            </div>
          </div>
        </div>

        <div class="accordion-item" id="create-farmer">
          <h2 class="accordion-header" id="headingThree">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
              Farmer Zone
            </button>
          </h2>
          <div id="collapseThree" class="accordion-collapse collapse" aria-labelledby="headingThree" data-bs-parent="#accordionExample">
            <div class="accordion-body">
<!--Join as farmer---------------------- ------------->
          <div class="row" id="mainrow">
            <div class="col-6">
                <h6>Join Area</h6>
                <hr class="text-primary border-1">
                <div class="mb-3">
                  <label for="exampleFormControlInput1" class="form-label">Area ID</label>
                  <input type="text" class="form-control" id="areaIDtoJoin" placeholder="1883">
                  <input id="currentFarmerCIDValueHidden" class="d-none" value=<%="#{@farmerdataurl}"%>>
                </div>
                <div class="d-grid gap-2 col-6 mx-auto">
                  <button class="btn btn-outline-primary" id="joinFarmerBtn" onClick="joinAsFarmer()" type="button">Join Area</button>
                </div>
            </div>
            <div class="col-6">
                <h6>Invite Farmer</h6>
                <hr class="text-primary border-1">
                <div class="mb-3">
                  <label for="exampleFormControlInput1" class="form-label">Address of farmer to invite</label>
                  <input type="text" class="form-control" id="inivtedFarmer" onChange="joinAreaValidate()" placeholder="0xE7b30A037F5598E4e73702ca66A59Af5CC650Dcd">
                </div>
                <div class="mb-3">
                  <label for="exampleFormControlInput1" class="form-label">ID of Area</label>
                  <input type="text" class="form-control" id="areaIDtoInvite" placeholder="1338">
                </div>
                <div class="d-grid gap-2 col-6 mx-auto">
                  <button class="btn btn-outline-primary" id="inviteFarmerBtn" onClick="inviteFarmer()" type="button">Invite Farmer</button>
                </div>

              </div>
            <div class="col-12">
                <h6>Create Area</h6>
                <hr class="text-primary border-1">
                <div class="mb-3">
                   <div class="mb-3">
                  <label for="area" class="form-label">Area Name</label>
                  <input type="text" class="form-control" id="areaName" placeholder="EthicHub-Comunidad El Progreso, 'Hasta Siempre'">
                </div>
                 <div class="mb-3">
                  <label for="area" class="form-label">Rule Contract</label>
                  <input type="text" class="form-control" onChange="ruleAreaValidate()" id="rulesForArea" placeholder="0x0000000000000000000000000000000000000000">
                </div>
                  <div class="mb-3">
                  <label for="area" class="form-label">Area Description</label>
                  <input type="textarea" class="form-control" id="areaDescription" placeholder="What is you and yo brethren all about?">
                </div>
                </div>
                <div class="d-grid gap-2 col-6 mx-auto">
                  <button class="btn btn-outline-primary" id="createAreaBtn" onClick="createArea()" type="button">Create Area</button>
                </div>
            </div>
          </div>

<!-- end---------------------- ------------->
            </div>
          </div>
        </div>

        <div class="accordion-item" id="create-area">
          <h2 class="accordion-header" id="headingFour">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseFour" aria-expanded="false" aria-controls="collapse">
              Consumer Zone
            </button>
          </h2>
          <div id="collapseFour" class="accordion-collapse collapse" aria-labelledby="headingFour" data-bs-parent="#accordionExample">
            <div class="accordion-body">
              <h6>Consumer Zone</h6>
                <hr class="text-primary border-1">
                <h5>soon ™</h5>
                <p>enough this section will include <br> the ability of a consumer to <br> join an area <br> as well as rejoyce in a list of all memberships and corresponding activity <br> such as area specific token balances</p>
            </div>
          </div>
        </div>

        <div class="accordion-item" id="create-basket">
          <h2 class="accordion-header" id="headingFive">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseFive" aria-expanded="false" aria-controls="collapseFive">
              Create Baskets
            </button>
          </h2>
          <div id="collapseFive" class="accordion-collapse collapse" aria-labelledby="headingFive" data-bs-parent="#accordionExample">
            <div class="accordion-body">
              <div class="col-12">
                 <h6>Mint Baskets</h6>
                <hr class="text-primary border-1">
                <div class="mb-3">
                  <label for="area" class="form-label">Area ID</label>
                  <input type="text" class="form-control" id="areaBasketID" placeholder="mint this as part of what area?">
                </div>
                 <div class="mb-3">
                  <label for="area" class="form-label">How many</label>
                  <input type="text" class="form-control" id="howMany" placeholder="101">
                </div>
                <div class="mb-3">
                  <label for="area" class="form-label">Price</label>
                  <input type="text" class="form-control" id="priceOfBasket">
                </div>
                <div class="mb-3">
                  <label for="area" class="form-label">Address of Denominationg Token</label>
                  <input type="text" class="form-control" onChange="erc20priceValidate()" id="ercAddress" placeholder="0x2ac65Be3f3aCA115Ae97669Dd3f0B8EA47A300Be">
                </div>
                <div class="row">
                <div class="col-6">
                  <div class="mb-3">
                    <label for="area" class="form-label">Redeamable from</label>
                    <input type="text" class="form-control" id="rFrom" placeholder="1610169969">
                  </div>
                </div>
                  <div class="col-6">
                    <div class="mb-3">
                      <label for="area" class="form-label">Redeamable until</label>
                      <input type="text" class="form-control" id="rUntil" placeholder="161969696">
                    </div>
                  </div>
                </div>
                </div>
                <div class="d-grid gap-2 col-6 mx-auto">
                  <button class="btn btn-outline-primary" id="createBasketBtn" onClick="createBaskets()" type="button">Create Basket</button>
                </div>
              </div>
            </div>
          </div>
        </div>
    </div>
  </div>
</div>
<div class="d-none">
